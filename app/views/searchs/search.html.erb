<% if @model == "User" %>
  <div>
    <h3>Users search for "<%= @content %>"</h3>
    <%= render 'users/index', users: @results %>
  </div>
<% elsif @model == "Book" %>
  <div>
    <h3>Books search for "<%= @content %>"</h3>
    <%= render 'books/index', books: @results %>
  </div>
<% elsif @model == "Tag" %>  
  <div>
    <h3>Books search for Tag:"<%= @content %>"</h3>
    <%= render 'books/index', books: @results %>
  </div>

<% end %>