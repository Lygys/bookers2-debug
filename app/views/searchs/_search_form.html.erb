<%= form_with(url:search_path, method: :get, local: true) do |f| %>
  <%= f.text_field :content %>
  <%= f.select :model, options_for_select([["User","User"], ["Book","Book"],["Tag","Tag"]]) %>
  <%= f.select :method, options_for_select([["完全一致","perfect"], ["部分一致","partial"], ["前方一致","forward"], ["後方一致","backward"]]) %>
  <%= f.submit '検索', class: "btn btn-primary" %>
<% end %>
